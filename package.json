{
	"name": "front-end-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"start": "node script/start.js",
		"start:vue": "vite packages/vue-template",
		"start:react": "vite packages/react-template",
		"test": "vitest --coverage.enabled --globals",
		"test:ui": "vitest --ui --open --coverage.enabled",
		"prepare": "husky",
		"lint:eslint": "pnpm run lint:eslint-vue & pnpm run lint:eslint-react",
		"lint:eslint-vue": "eslint --config packages/vue-template/eslint.config.mjs --cache --fix",
		"lint:eslint-react": "eslint --config packages/react-template/eslint.config.mjs --cache --fix",
		"lint:format": "prettier --write --log-level warn \"packages/**/src/**/*.{js,ts,json,jsx,tsx,css,sass,less,vue,html,md}\"",
		"lint:stylelint": "pnpm run lint:stylelint-vue & pnpm run lint:stylelint-react",
		"lint:stylelint-vue": "stylelint \"**/vue-template/**/*.{css,scss,vue,html}\" --cache --fix --config packages/vue-template/stylelint.config.mjs",
		"lint:stylelint-react": "stylelint \"**/react-template/**/*.{css,scss,vue,html}\" --cache --fix --config packages/react-template/stylelint.config.mjs",
		"clean": "rimraf -g \"**/node_modules\" & pnpm store prune"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@9.0.6+sha1.648f6014eb363abb36618f2ba59282a9eeb3e879",
	"dependencies": {
		"inquirer": "^9.2.20",
		"rimraf": "^5.0.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/js": "^9.1.1",
		"@types/node": "^20.12.10",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"@vitest/coverage-v8": "^1.6.0",
		"@vitest/ui": "^1.6.0",
		"commitlint": "^19.3.0",
		"core-js": "^3.30.1",
		"eslint": "^9.1.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-define-config": "^2.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"globals": "^15.1.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"postcss-scss": "^4.0.9",
		"prettier": "3.2.5",
		"rollup-plugin-visualizer": "^5.12.0",
		"stylelint": "^16.4.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^5.0.1",
		"stylelint-config-standard": "^36.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5",
		"unocss": "^0.59.4",
		"unplugin-auto-import": "^0.17.5",
		"vite": "^5.2.11",
		"vite-plugin-compression2": "^1.1.0",
		"vite-plugin-progress": "^0.0.7",
		"vitest": "^1.6.0"
	},
	"lint-staged": {
		"**/vue-template/**/*.{js,ts,jsx,tsx,vue}": [
			"pnpm run lint:eslint-vue",
			"pnpm run lint:format"
		],
		"**/react-template/**/*.{js,ts,jsx,tsx}": [
			"pnpm run lint:eslint-react",
			"pnpm run lint:format"
		],
		"**/vue-template/**/*.{css,scss,sass,less,vue,html}": [
			"pnpm run lint:stylelint-vue"
		],
		"**/react-template/**/*.{css,scss,sass,less,vue,html}": [
			"pnpm run lint:stylelint-react"
		],
		"*.test.{js,jsx,ts,tsx}": [
			"vitest related --run --globals"
		]
	}
}
