{
  "name": "front-end-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "lint:eslint": "eslint --cache --fix",
    "lint:eslint-vue": "eslint --config packages/vue-template/eslint.config.mjs --cache --fix",
    "lint:eslint-react": "eslint --config packages/react-template/eslint.config.mjs --cache --fix",
    "lint:format": "prettier --write --log-level warn \"src/**/*.{js,ts,json,jsx,tsx,css,sass,less,vue,html,md}\"",
    "lint:stylelint": "stylelint \"**/*.{css,scss,vue,html}\" --cache --fix",
    "lint:stylelint-vue": "stylelint \"**/*.{css,scss,vue,html}\" --cache --fix --config packages/vue-template/stylelint.config.mjs",
    "lint:stylelint-react": "stylelint \"**/*.{css,scss,vue,html}\" --cache --fix --config packages/react-template/stylelint.config.mjs",
    "start:vue": "farm packages/vue-template",
    "start:react": "farm packages/react-template"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.0.6+sha1.648f6014eb363abb36618f2ba59282a9eeb3e879",
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@farmfe/cli": "^1.0.1",
    "@farmfe/core": "^1.1.1",
    "@farmfe/plugin-sass": "^1.0.3",
    "core-js": "^3.30.1",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "stylelint": "^16.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript-eslint": "^7.7.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
    {
      "files": ["vue-template/**/*.{js,vue,jsx}"],
      "command": "pnpm run lint:eslint-vue"
    },
    {
      "files": ["react-template/**/*.{js,vue,jsx}"],
      "command": "pnpm run lint:eslint-react"
    },
      "pnpm run lint:format"
    ],
    "*.{css,scss,sass,less,vue,html}": [
      "pnpm run lint:stylelint"
    ]
  }
}
